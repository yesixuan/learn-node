doctype
html
  head(lang="en")
    meta(charset="UTF-8")
    title jade页面
    style.
      div {width:100px;height:100px;background:#ccc;text-align:center;float:left;margin:10px auto;}
      div.last {clear:left}
  body
    -var a=0
    while a<12
      if a%4==0 && a!=0
       div.last #{a++}
      else
        div #{a++}
